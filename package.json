    "chai": "^3.5.0",
    "mocha": "^3.2.0",
    "awesome-typescript-loader": "^3.0.0-beta.17",
    "source-map-loader": "^0.1.5",
    "tslint": "^4.2.0",
    "tslint-loader": "^3.3.0",
    "typescript": "^2.1.4",
    "webpack": "^1.14.0",
    "yargs": "^6.4.0"
  }
